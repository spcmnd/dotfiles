URxvt*font: xft:DejaVuSansMono Nerd Font Mono:style=Regular:size=9
URxvt*boldFont: xft:DejaVuSansMono Nerd Font Mono:style=Bold:size=9
URxvt*italicFont: xft:DejaVuSansMono Nerd Font Mono:style=Italic:size=9
URxvt*boldItalicFont: xft:DejaVuSansMono Nerd Font Mono:style=Bold Italic:size=9
URxvt*letterSpace: 0
URxvt*lineSpace: 0
URxvt*geometry: 92x24
URxvt*internalBorder: 8
URxvt*cursorBlink: true
URxvt*cursorUnderline: false
URxvt*saveline: 2048
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*urgentOnBell: true
URxvt*depth: 24
URxvt*iso14755: false

URxvt*urlLauncher: firefox
URxvt*underlineURLs: true
URxvt*urlButton: 1

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:

